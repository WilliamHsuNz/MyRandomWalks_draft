            <taxon id="t38" spec="Taxon"/>
            <taxon id="t39" spec="Taxon"/>
            <taxon id="t40" spec="Taxon"/>
            <taxon id="t41" spec="Taxon"/>
            <taxon id="t42" spec="Taxon"/>
            <taxon id="t43" spec="Taxon"/>
            <taxon id="t44" spec="Taxon"/>
