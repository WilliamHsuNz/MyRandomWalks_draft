    <!--data id="alignment" dataType="nucleotide">
         <sequence taxon="t0">?</sequence>
        <sequence taxon="t1">?</sequence>
        <sequence taxon="t2">?</sequence>
        <sequence taxon="t3">?</sequence>
        <sequence taxon="t4">?</sequence>
        <sequence taxon="t5">?</sequence>
    </data-->
